// COM: Must use a separate FileCheck for each instantiated version of the structs because
// COM:   there is no guarantee on the order they will be produced. The final FileCheck
// COM:   runs on the last component that uses the default "CHECK" prefix.
//
// RUN: llzk-opt -llzk-flatten %s > %t \
// RUN:   && FileCheck %s --check-prefix CHECK1 < %t \
// RUN:   && FileCheck %s --check-prefix CHECK2 < %t \
// RUN:   && FileCheck %s --check-prefix CHECK3 < %t \
// RUN:   && FileCheck %s < %t

module attributes {veridise.lang = "llzk"} {
  llzk.struct @Component08A<[@A, @B]> {
    function.def @compute() -> !llzk.struct<@Component08A<[@A, @B]>> {
      %self = llzk.new_struct : !llzk.struct<@Component08A<[@A, @B]>>
      function.return %self : !llzk.struct<@Component08A<[@A, @B]>>
    }
    function.def @constrain(%self: !llzk.struct<@Component08A<[@A, @B]>>) { function.return }
  }

  llzk.struct @Component08B<[@C]> {
    llzk.field @f2 : !llzk.struct<@Component08A<[5, @C]>>

    function.def @compute() -> !llzk.struct<@Component08B<[@C]>> {
      %self = llzk.new_struct : !llzk.struct<@Component08B<[@C]>>
      %x = function.call @Component08A::@compute() : () -> (!llzk.struct<@Component08A<[5, @C]>>)
      llzk.writef %self[@f2] = %x : !llzk.struct<@Component08B<[@C]>>, !llzk.struct<@Component08A<[5, @C]>>
      function.return %self : !llzk.struct<@Component08B<[@C]>>
    }

    function.def @constrain(%self: !llzk.struct<@Component08B<[@C]>>) {
      %b = llzk.readf %self[@f2] : !llzk.struct<@Component08B<[@C]>>, !llzk.struct<@Component08A<[5, @C]>>
      function.call @Component08A::@constrain(%b) : (!llzk.struct<@Component08A<[5, @C]>>) -> ()
      function.return
    }
  }

  llzk.struct @Component08C {
    llzk.field @f3 : !llzk.struct<@Component08B<[7]>>
    llzk.field @f4 : !llzk.struct<@Component08B<[!felt.felt]>>
    llzk.field @f5 : !llzk.struct<@Component08B<[7]>>
    llzk.field @f6 : !llzk.struct<@Component08B<[5]>>

    function.def @compute() -> !llzk.struct<@Component08C> {
      %self = llzk.new_struct : !llzk.struct<@Component08C>

      %1 = function.call @Component08B::@compute() : () -> (!llzk.struct<@Component08B<[7]>>)
      llzk.writef %self[@f3] = %1 : !llzk.struct<@Component08C>, !llzk.struct<@Component08B<[7]>>

      %2 = function.call @Component08B::@compute() : () -> (!llzk.struct<@Component08B<[!felt.felt]>>)
      llzk.writef %self[@f4] = %2 : !llzk.struct<@Component08C>, !llzk.struct<@Component08B<[!felt.felt]>>

      %3 = function.call @Component08B::@compute() : () -> (!llzk.struct<@Component08B<[7]>>)
      llzk.writef %self[@f5] = %3 : !llzk.struct<@Component08C>, !llzk.struct<@Component08B<[7]>>

      %4 = function.call @Component08B::@compute() : () -> (!llzk.struct<@Component08B<[5]>>)
      llzk.writef %self[@f6] = %4 : !llzk.struct<@Component08C>, !llzk.struct<@Component08B<[5]>>

      function.return %self : !llzk.struct<@Component08C>
    }

    function.def @constrain(%self: !llzk.struct<@Component08C>) {
      %1 = llzk.readf %self[@f3] : !llzk.struct<@Component08C>, !llzk.struct<@Component08B<[7]>>
      function.call @Component08B::@constrain(%1) : (!llzk.struct<@Component08B<[7]>>) -> ()

      %2 = llzk.readf %self[@f4] : !llzk.struct<@Component08C>, !llzk.struct<@Component08B<[!felt.felt]>>
      function.call @Component08B::@constrain(%2) : (!llzk.struct<@Component08B<[!felt.felt]>>) -> ()

      %3 = llzk.readf %self[@f5] : !llzk.struct<@Component08C>, !llzk.struct<@Component08B<[7]>>
      function.call @Component08B::@constrain(%3) : (!llzk.struct<@Component08B<[7]>>) -> ()

      %4 = llzk.readf %self[@f6] : !llzk.struct<@Component08C>, !llzk.struct<@Component08B<[5]>>
      function.call @Component08B::@constrain(%4) : (!llzk.struct<@Component08B<[5]>>) -> ()

      function.return
    }
  }
}
