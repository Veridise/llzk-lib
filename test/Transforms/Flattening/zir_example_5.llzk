// CHECK-NEXT:    function.def @compute(%[[V0:[0-9a-zA-Z_\.]+]]: !array.array<4 x !llzk.felt>) -> !llzk.struct<@Unpack_8_2_4> {
// CHECK-NEXT:      %[[V1:[0-9a-zA-Z_\.]+]] = arith.constant 0 : index
// CHECK-NEXT:      %[[V2:[0-9a-zA-Z_\.]+]] = new_struct : <@Unpack_8_2_4>
// CHECK-NEXT:      %[[V3:[0-9a-zA-Z_\.]+]] = array.read %[[V0]]{{\[}}%[[V1]]] : <4 x !llzk.felt>, !llzk.felt
// CHECK-NEXT:      function.return %[[V2]] : !llzk.struct<@Unpack_8_2_4>
// CHECK-NEXT:    }
// CHECK-NEXT:    function.def @constrain(%[[V4:[0-9a-zA-Z_\.]+]]: !llzk.struct<@Unpack_8_2_4>, %[[V5:[0-9a-zA-Z_\.]+]]: !array.array<4 x !llzk.felt>) attributes {function.allow_constraints = #function.allow_constraints} {
// CHECK-NEXT:      function.return
// CHECK-NEXT:    }
// CHECK-NEXT:  }
//
// CHECK-LABEL: llzk.struct @User1 {
// CHECK-NEXT:    function.def @compute() -> !llzk.struct<@User1> {
// CHECK-NEXT:      %[[V0:[0-9a-zA-Z_\.]+]] = constfelt 42
// CHECK-NEXT:      %[[V1:[0-9a-zA-Z_\.]+]] = array.new %[[V0]], %[[V0]], %[[V0]], %[[V0]] : <4 x !llzk.felt>
// CHECK-NEXT:      %[[V2:[0-9a-zA-Z_\.]+]] = function.call @Unpack_8_2_4::@compute(%[[V1]]) : (!array.array<4 x !llzk.felt>) -> !llzk.struct<@Unpack_8_2_4>
// CHECK-NEXT:      %[[V3:[0-9a-zA-Z_\.]+]] = new_struct : <@User1>
// CHECK-NEXT:      function.return %[[V3]] : !llzk.struct<@User1>
// CHECK-NEXT:    }
// CHECK-NEXT:    function.def @constrain(%[[V4:[0-9a-zA-Z_\.]+]]: !llzk.struct<@User1>) attributes {function.allow_constraints = #function.allow_constraints} {
// CHECK-NEXT:      function.return
// CHECK-NEXT:    }
// CHECK-NEXT:  }
