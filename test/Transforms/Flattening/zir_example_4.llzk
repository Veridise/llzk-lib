// RUN: llzk-opt -split-input-file -llzk-flatten -verify-diagnostics %s | FileCheck --enable-var-scope %s

#map = affine_map<()[s0, s1] -> (s0 floordiv s1)>
module attributes {veridise.lang = "llzk"} {
  llzk.struct @S<[@X, @N, @P]> {
    function.def @compute(%arg0: !array.array<@X x !felt.felt>) -> !llzk.struct<@S<[@X, @N, @P]>> {
      %self = new_struct : !llzk.struct<@S<[@X, @N, @P]>>
      %c0 = arith.constant 0 : index
      %0 = array.read %arg0[%c0] : !array.array<@X x !felt.felt>, !felt.felt
      function.return %self : !llzk.struct<@S<[@X, @N, @P]>>
    }
    function.def @constrain(%arg0: !llzk.struct<@S<[@X, @N, @P]>>, %arg1: !array.array<@X x !felt.felt>) {
      function.return
    }
  }
  llzk.struct @B<[@N]> {
    function.def @compute() -> !llzk.struct<@B<[@N]>> {
      %self = new_struct : !llzk.struct<@B<[@N]>>
      function.return %self : !llzk.struct<@B<[@N]>>
    }
    function.def @constrain(%arg0: !llzk.struct<@B<[@N]>>) {
      function.return
    }
  }
  llzk.struct @A<[@N]> {
    function.def @compute() -> !llzk.struct<@A<[@N]>> {
      %self = new_struct : !llzk.struct<@A<[@N]>>
      %0 = read_const @N : index
      %c2 = arith.constant 2 : index
      %array = array.new{()[%0, %c2]} : !array.array<#map x !felt.felt>
      %1 = function.call @S::@compute(%array) {()[%0, %c2]} : (!array.array<#map x !felt.felt>) -> !llzk.struct<@S<[#map, @N, 2]>>
      %2 = function.call @B::@compute() {()[%0, %c2]} : () -> !llzk.struct<@B<[#map]>>
      function.return %self : !llzk.struct<@A<[@N]>>
    }
    function.def @constrain(%arg0: !llzk.struct<@A<[@N]>>) {
      function.return
    }
  }
  llzk.struct @Main {
    function.def @compute() -> !llzk.struct<@Main> {
      %s = function.call @A::@compute() : () -> !llzk.struct<@A<[4]>>
      %self = new_struct : !llzk.struct<@Main>
      function.return %self : !llzk.struct<@Main>
    }
    function.def @constrain(%arg0: !llzk.struct<@Main>) {
      function.return
    }
  }
}
