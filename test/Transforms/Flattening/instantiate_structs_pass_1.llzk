  struct.def @Component02<[@A]> {
    function.def @compute(%0: !felt.felt) -> !struct.struct<@Component02<[@A]>> {
      %self = struct.new : !struct.struct<@Component02<[@A]>>
      function.return %self : !struct.struct<@Component02<[@A]>>
    }
    function.def @constrain(%self: !struct.struct<@Component02<[@A]>>, %0: !felt.felt) {
      %1 = llzk.read_const @A : !felt.felt
      llzk.emit_eq %0, %1 : !felt.felt
      function.return
    }
  }
}
