module attributes {veridise.lang = "llzk"} {
  struct.def @Component01A<[@A, @B]> {
    function.def @compute() -> !struct.struct<@Component01A<[@A, @B]>> {
      %self = struct.new : !struct.struct<@Component01A<[@A, @B]>>
      function.return %self : !struct.struct<@Component01A<[@A, @B]>>
    }
    function.def @constrain(%self: !struct.struct<@Component01A<[@A, @B]>>) { function.return }
  }

  struct.def @Component01B<[@C]> {
    struct.field @f2 : !struct.struct<@Component01A<[5, @C]>>

    function.def @compute() -> !struct.struct<@Component01B<[@C]>> {
      %self = struct.new : !struct.struct<@Component01B<[@C]>>
      %x = function.call @Component01A::@compute() : () -> (!struct.struct<@Component01A<[5, @C]>>)
      struct.writef %self[@f2] = %x : !struct.struct<@Component01B<[@C]>>, !struct.struct<@Component01A<[5, @C]>>
      function.return %self : !struct.struct<@Component01B<[@C]>>
    }

    function.def @constrain(%self: !struct.struct<@Component01B<[@C]>>) {
      %b = struct.readf %self[@f2] : !struct.struct<@Component01B<[@C]>>, !struct.struct<@Component01A<[5, @C]>>
      function.call @Component01A::@constrain(%b) : (!struct.struct<@Component01A<[5, @C]>>) -> ()
      function.return
    }
  }

  struct.def @Component01C {
    struct.field @f3 : !struct.struct<@Component01B<[43]>>

    function.def @compute() -> !struct.struct<@Component01C> {
      %self = struct.new : !struct.struct<@Component01C>
      %x = function.call @Component01B::@compute() : () -> (!struct.struct<@Component01B<[43]>>)
      struct.writef %self[@f3] = %x : !struct.struct<@Component01C>, !struct.struct<@Component01B<[43]>>
      function.return %self : !struct.struct<@Component01C>
    }

    function.def @constrain(%self: !struct.struct<@Component01C>) {
      %b = struct.readf %self[@f3] : !struct.struct<@Component01C>, !struct.struct<@Component01B<[43]>>
      function.call @Component01B::@constrain(%b) : (!struct.struct<@Component01B<[43]>>) -> ()
      function.return
    }
  }
}
