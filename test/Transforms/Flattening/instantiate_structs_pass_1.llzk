// RUN: llzk-opt -I %S -split-input-file -llzk-flatten -verify-diagnostics %s | FileCheck --enable-var-scope %s

/// Test calling a global function named "compute"; should have no change
module attributes {veridise.lang = "llzk"} {
  function.def @compute(%x: i1) -> i1 {
    function.return %x : i1
  }
  function.def @global_caller(%a: i1, %b: i1) -> i1 {
    %1 = function.call @compute(%a) : (i1) -> i1
    function.return %1 : i1
  }
  struct.def @StructCaller {
    function.def @compute(%a: i1) -> !struct.struct<@StructCaller> {
      %self = struct.new : !struct.struct<@StructCaller>
      %1 = function.call @compute(%a) : (i1) -> i1
      function.return %self : !struct.struct<@StructCaller>
    }
    function.def @constrain(%self: !struct.struct<@StructCaller>, %a: i1) {
      function.return
    }
  }
}
// CHECK-LABEL: function.def @compute(
// CHECK-SAME:                     %[[VAL_0:[0-9a-zA-Z_\.]+]]: i1) -> i1 {
// CHECK-NEXT:    function.return %[[VAL_0]] : i1
// CHECK-NEXT:  }
//
// CHECK-LABEL: function.def @global_caller(
// CHECK-SAME:                           %[[VAL_0:[0-9a-zA-Z_\.]+]]: i1,
// CHECK-SAME:                           %[[VAL_1:[0-9a-zA-Z_\.]+]]: i1) -> i1 {
// CHECK-NEXT:    %[[VAL_2:[0-9a-zA-Z_\.]+]] = function.call @compute(%[[VAL_0]]) : (i1) -> i1
// CHECK-NEXT:    function.return %[[VAL_2]] : i1
// CHECK-NEXT:  }
// -----

/// Test calling a struct "compute" without params; should have no change
module attributes {veridise.lang = "llzk"} {
  struct.def @StructA {
    function.def @compute() -> !struct.struct<@StructA> {
      %self = struct.new : !struct.struct<@StructA>
      function.return %self : !struct.struct<@StructA>
    }
    function.def @constrain(%self: !struct.struct<@StructA>) {
      function.return
    }
  }
  struct.def @StructCaller {
    function.def @compute() -> !struct.struct<@StructCaller> {
      %self = struct.new : !struct.struct<@StructCaller>
      %1 = function.call @StructA::@compute() : () -> !struct.struct<@StructA>
      function.return %self : !struct.struct<@StructCaller>
    }
    function.def @constrain(%self: !struct.struct<@StructCaller>) {
      function.return
    }
  }
}
