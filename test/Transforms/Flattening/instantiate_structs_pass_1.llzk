module attributes {veridise.lang = "llzk"} {
  struct.def @Component05<[@S, @T]> {
    struct.field @f : !array.array<@S x !llzk.tvar<@T>>

    function.def @compute() -> !struct.struct<@Component05<[@S, @T]>> {
      %self = struct.new : !struct.struct<@Component05<[@S, @T]>>
      %arr = array.new : !array.array<@S x !llzk.tvar<@T>>
      %lb = arith.constant 0 : index
      %up = llzk.read_const @S : index
      %step = arith.constant 1 : index
      scf.for %iv = %lb to %up step %step {
        // Although a type variable is used, this struct can only be instantiated
        //  with `@T = index` since an `index` value is assigned to the tvar array.
        array.write %arr[%iv] = %iv : !array.array<@S x !llzk.tvar<@T>>, index
      }
      struct.writef %self[@f] = %arr : !struct.struct<@Component05<[@S, @T]>>, !array.array<@S x !llzk.tvar<@T>>
      function.return %self : !struct.struct<@Component05<[@S, @T]>>
    }

    function.def @constrain(%self: !struct.struct<@Component05<[@S, @T]>>) {
      %arr = struct.readf %self[@f] : !struct.struct<@Component05<[@S, @T]>>, !array.array<@S x !llzk.tvar<@T>>
      %lb = arith.constant 0 : index
      %up = llzk.read_const @S : index
      %step = arith.constant 1 : index
      scf.for %iv = %lb to %up step %step {
        %temp = array.read %arr[%iv] : !array.array<@S x !llzk.tvar<@T>>, !llzk.tvar<@T>
        llzk.emit_eq %temp, %iv : !llzk.tvar<@T>, index // requires that @T==index
      }
      function.return
    }
  }
  struct.def @Main {
    struct.field @sub : !struct.struct<@Component05<[5, index]>>

    function.def @compute() -> !struct.struct<@Main> {
      %self = struct.new : !struct.struct<@Main>
      %1 = function.call @Component05::@compute() : () -> !struct.struct<@Component05<[5, index]>>
      struct.writef %self[@sub] = %1 : !struct.struct<@Main>, !struct.struct<@Component05<[5, index]>>
      function.return %self : !struct.struct<@Main>
    }
    function.def @constrain(%self: !struct.struct<@Main>) {
      %1 = struct.readf %self[@sub] : !struct.struct<@Main>, !struct.struct<@Component05<[5, index]>>
      function.call @Component05::@constrain(%1) : (!struct.struct<@Component05<[5, index]>>) -> ()
      function.return
    }
  }
}
