module attributes {veridise.lang = "llzk"} {
  llzk.struct @ArrayConstraint {

    llzk.field @arr : !array.array<3 x !felt.felt>

    function.def @compute() -> !llzk.struct<@ArrayConstraint> {
      %self = new_struct : !llzk.struct<@ArrayConstraint>
      function.return %self : !llzk.struct<@ArrayConstraint>
    }

    function.def @constrain(%self: !llzk.struct<@ArrayConstraint>) {
      %5 = felt.const 5
      %7 = felt.const 7
      %11 = felt.const 11
      %arr = readf %self[@arr] : !llzk.struct<@ArrayConstraint>, !array.array<3 x !felt.felt>
      %i0 = arith.constant 0 : index
      %i1 = arith.constant 1 : index
      %i2 = arith.constant 2 : index
      %a = array.read %arr[%i0] : !array.array<3 x !felt.felt>, !felt.felt
      %b = array.read %arr[%i1] : !array.array<3 x !felt.felt>, !felt.felt
      %c = array.read %arr[%i2] : !array.array<3 x !felt.felt>, !felt.felt
      %cmp1 = cmp ge(%a, %5)
      assert %cmp1
      %cmp2 = cmp le(%a, %7)
      assert %cmp2
      %cmp3 = cmp ge(%b, %7)
      assert %cmp3
      %cmp4 = cmp le(%b, %11)
      assert %cmp4

      emit_eq %c, %a : !felt.felt

      function.return
    }
  }
}

// CHECK-LABEL: @ArrayConstraint StructIntervals {
// CHECK-NEXT:     %arg0[@arr][0] in TypeA:[ 5, 7 ]
// CHECK-NEXT:     %arg0[@arr][1] in TypeA:[ 7, 11 ]
// CHECK-NEXT:     %arg0[@arr][2] in TypeA:[ 5, 7 ]
// CHECK-NEXT: }

