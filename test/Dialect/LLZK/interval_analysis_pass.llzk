module attributes {veridise.lang = "llzk"} {
  llzk.struct @FieldRangeConstraint {

    llzk.field @foo : !felt.felt
    llzk.field @bar : !felt.felt

    function.def @compute(%a: !felt.felt, %b: !felt.felt) -> !llzk.struct<@FieldRangeConstraint> {
      %self = new_struct : !llzk.struct<@FieldRangeConstraint>
      function.return %self : !llzk.struct<@FieldRangeConstraint>
    }

    function.def @constrain(%self: !llzk.struct<@FieldRangeConstraint>, %a: !felt.felt, %b: !felt.felt) {
      %x = felt.const 1
      %y = felt.const 2
      %cmpA = cmp gt(%a, %x)
      assert %cmpA
      %cmpB = cmp le(%b, %y)
      assert %cmpB
      %foo = readf %self[@foo] : !llzk.struct<@FieldRangeConstraint>, !felt.felt
      emit_eq %foo, %x : !felt.felt
      %bar = readf %self[@bar] : !llzk.struct<@FieldRangeConstraint>, !felt.felt
      emit_eq %bar, %b : !felt.felt
      function.return
    }
  }
}

// CHECK-LABEL: @FieldRangeConstraint StructIntervals {
// CHECK-NEXT:     %arg0[@foo] in Degenerate(1)
// CHECK-NEXT:     %arg0[@bar] in TypeA:[ 0, 2 ]
// CHECK-NEXT:     %arg1 in TypeC:[ 2, 21888242871839275222246405745257275088696311157297823662689037894645226208582 ]
// CHECK-NEXT:     %arg2 in TypeA:[ 0, 2 ]
// CHECK-NEXT: }

