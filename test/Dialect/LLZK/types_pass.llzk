// RUN: llzk-opt -split-input-file %s 2>&1 | FileCheck --enable-var-scope %s

module attributes {veridise.lang = "llzk"} {
  struct.def @Hello {
    function.def @compute() -> !struct.struct<@Hello> {
      %self = struct.new : !struct.struct<@Hello>
      function.return %self : !struct.struct<@Hello>
    }
    function.def @constrain(%self: !struct.struct<@Hello>) { function.return }
  }
  function.def private @a(!array.array<5 x !felt.felt>, !struct.struct<@Hello>)
}
//CHECK-LABEL:  function.def private @a(!array.array<5 x !felt.felt>, !struct.struct<@Hello>)
// -----

function.def private @b(!array.array<3,2,1 x !felt.felt>)
