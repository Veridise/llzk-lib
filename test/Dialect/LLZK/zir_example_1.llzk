// _RUN_: llzk-opt -I %input_dir -llzk-print-call-graph-sccs %s 2>&1 | FileCheck --enable-var-scope %s --check-prefix SCC

module attributes {veridise.lang = "llzk"} {
  include.from "Dialect/LLZK/zir_example_0.llzk" as @std

  llzk.struct @C1 {
    llzk.field @z : !llzk.struct<@std::@risc0::@Reg> {llzk.pub}

    function.def @compute(%x: !felt.felt, %y: !felt.felt) -> !llzk.struct<@C1> {
      %self = llzk.new_struct : !llzk.struct<@C1>
      %add_0 = felt.add %x, %y
      %reg_0 = function.call @std::@risc0::@Reg::@compute(%add_0) : (!felt.felt) -> (!llzk.struct<@std::@risc0::@Reg>)
      llzk.writef %self[@z] = %reg_0 : !llzk.struct<@C1>, !llzk.struct<@std::@risc0::@Reg>
      function.return %self: !llzk.struct<@C1>
    }

    function.def @constrain(%self: !llzk.struct<@C1>, %x: !felt.felt, %y: !felt.felt) {
      %reg_0 = llzk.readf %self[@z] : !llzk.struct<@C1>, !llzk.struct<@std::@risc0::@Reg>
      %add_0 = felt.add %x, %y
      function.call @std::@risc0::@Reg::@constrain(%reg_0, %add_0) : (!llzk.struct<@std::@risc0::@Reg>, !felt.felt) -> ()
      function.return
    }
  }

  llzk.struct @C2 {
    llzk.field @fc1 : !llzk.struct<@C1>

    function.def @compute(%x: !llzk.struct<@std::@risc0::@ValU32>) -> !llzk.struct<@C2> {
      %self = llzk.new_struct : !llzk.struct<@C2>
      %t1 = llzk.readf %x[@low] : !llzk.struct<@std::@risc0::@ValU32>, !felt.felt
      %t2 = llzk.readf %x[@high] : !llzk.struct<@std::@risc0::@ValU32>, !felt.felt
      %t0 = function.call @C1::@compute(%t1, %t2) : (!felt.felt, !felt.felt) -> (!llzk.struct<@C1>)
      llzk.writef %self[@fc1] = %t0 : !llzk.struct<@C2>, !llzk.struct<@C1>
      //
      function.return %self: !llzk.struct<@C2>
    }

    function.def @constrain(%self: !llzk.struct<@C2>, %x: !llzk.struct<@std::@risc0::@ValU32>) {
      // c1.constrain(x.low, x.high)
      %t0 = llzk.readf %self[@fc1] : !llzk.struct<@C2>, !llzk.struct<@C1>
      %t1 = llzk.readf %x[@low] : !llzk.struct<@std::@risc0::@ValU32>, !felt.felt
      %t2 = llzk.readf %x[@high] : !llzk.struct<@std::@risc0::@ValU32>, !felt.felt
      function.call @C1::@constrain(%t0, %t1, %t2) : (!llzk.struct<@C1>, !felt.felt, !felt.felt) -> ()
      // emit c1.z.reg = 0
      %t3 = llzk.readf %t0[@z] : !llzk.struct<@C1>, !llzk.struct<@std::@risc0::@Reg>
      %t4 = llzk.readf %t3[@reg] : !llzk.struct<@std::@risc0::@Reg>, !felt.felt
      %zero = felt.const 0
      llzk.emit_eq %t4, %zero : !felt.felt
      //
      function.return
    }
  }
}

