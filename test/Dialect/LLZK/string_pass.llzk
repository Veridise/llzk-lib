// RUN: llzk-opt -split-input-file %s 2>&1 | FileCheck %s

module attributes {veridise.lang = "llzk"} {
  struct.def @Foo<[]> {
    function.def @compute() -> !struct.struct<@Foo<[]>> {
      %self = struct.new : !struct.struct<@Foo<[]>>
      %str = string.new "this is a string"
      function.return %self : !struct.struct<@Foo<[]>>
    }
    function.def @constrain(%self: !struct.struct<@Foo<[]>>) { function.return }
  }
}
