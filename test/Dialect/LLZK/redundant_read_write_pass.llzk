module attributes {veridise.lang = "llzk"} {
  struct.def @Component {
    struct.field @arr : !array.array<3 x !felt.felt>

    function.def @compute(%arg : !felt.felt) -> !struct.struct<@Component> {
      %self = struct.new : !struct.struct<@Component>
      %arr = struct.readf %self[@arr] : !struct.struct<@Component>, !array.array<3 x !felt.felt>
      %idx = arith.constant 1 : index
      array.write %arr[%idx] = %arg : !array.array<3 x !felt.felt>, !felt.felt
      %newarr = array.new %arg, %arg, %arg : !array.array<3 x !felt.felt> // ends up being unused after removing the below ops, can also be removed
      %elem = array.read %newarr[%idx] : !array.array<3 x !felt.felt>, !felt.felt // this is just %arg, redundant
      array.write %arr[%idx] = %elem : !array.array<3 x !felt.felt>, !felt.felt // redundant write, this idx is already %arg
      function.return %self : !struct.struct<@Component>
    }
    function.def @constrain(%self: !struct.struct<@Component>, %arg : !felt.felt) { function.return }
  }
}

