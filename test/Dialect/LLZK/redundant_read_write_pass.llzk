// RUN: llzk-opt -split-input-file -llzk-duplicate-read-write-elim %s 2>&1 | FileCheck %s

module attributes {veridise.lang = "llzk"} {

  struct.def @Subcomponent {
    function.def @compute() -> !struct.struct<@Subcomponent> {
      %self = struct.new : !struct.struct<@Subcomponent>
      function.return %self : !struct.struct<@Subcomponent>
    }
    function.def @constrain(%self: !struct.struct<@Subcomponent>) { function.return }
  }

  struct.def @Component {
    struct.field @sub1 : !struct.struct<@Subcomponent>
    struct.field @sub2 : !struct.struct<@Subcomponent>

    function.def @compute() -> !struct.struct<@Component> {
      %self = struct.new : !struct.struct<@Component>
      // All these reads are unused, so they will be removed
      %sub1 = struct.readf %self[@sub1] : !struct.struct<@Component>, !struct.struct<@Subcomponent>
      %sub2 = struct.readf %self[@sub2] : !struct.struct<@Component>, !struct.struct<@Subcomponent>
      %sub3 = struct.readf %self[@sub2] : !struct.struct<@Component>, !struct.struct<@Subcomponent>
      function.return %self : !struct.struct<@Component>
    }
    function.def @constrain(%self: !struct.struct<@Component>) { function.return }
  }
}

