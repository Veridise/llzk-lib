module attributes {veridise.lang = "llzk"} {
  llzk.struct @Component {
    field @arr : !array.array<3 x !llzk.felt>

    function.def @compute(%arg : !llzk.felt) -> !llzk.struct<@Component> {
      %self = new_struct : !llzk.struct<@Component>
      %arr = readf %self[@arr] : !llzk.struct<@Component>, !array.array<3 x !llzk.felt>
      %idx = arith.constant 1 : index
      array.write %arr[%idx] = %arg : !array.array<3 x !llzk.felt>, !llzk.felt
      %newarr = array.new %arg, %arg, %arg : !array.array<3 x !llzk.felt> // ends up being unused after removing the below ops, can also be removed
      %elem = array.read %newarr[%idx] : !array.array<3 x !llzk.felt>, !llzk.felt // this is just %arg, redundant
      array.write %arr[%idx] = %elem : !array.array<3 x !llzk.felt>, !llzk.felt // redundant write, this idx is already %arg
      function.return %self : !llzk.struct<@Component>
    }
    function.def @constrain(%self: !llzk.struct<@Component>, %arg : !llzk.felt) { function.return }
  }
}

