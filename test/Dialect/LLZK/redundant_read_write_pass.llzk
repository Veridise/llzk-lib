module attributes {veridise.lang = "llzk"} {
  struct.def @Component {
    struct.field @arr : !array.array<3 x !felt.felt>

    function.def @compute(%arg : !felt.felt, %inputIdx : index) -> !struct.struct<@Component> {
      %self = struct.new : !struct.struct<@Component>
      %arr = struct.readf %self[@arr] : !struct.struct<@Component>, !array.array<3 x !felt.felt>
      %idx = arith.constant 1 : index
      %idx2 = arith.constant 2 : index
      array.write %arr[%idx] = %arg : !array.array<3 x !felt.felt>, !felt.felt
      array.write %arr[%inputIdx] = %arg : !array.array<3 x !felt.felt>, !felt.felt
      // this read is necessary in the case that %inputIdx == %idx
      %elem1 = array.read %arr[%idx] : !array.array<3 x !felt.felt>, !felt.felt
      %elem2 = array.read %arr[%inputIdx] : !array.array<3 x !felt.felt>, !felt.felt
      array.write %arr[%idx] = %elem2 : !array.array<3 x !felt.felt>, !felt.felt
      array.write %arr[%idx2] = %elem1 : !array.array<3 x !felt.felt>, !felt.felt
      function.return %self : !struct.struct<@Component>
    }
    function.def @constrain(%self: !struct.struct<@Component>, %arg : !felt.felt, %inputIdx : index) { function.return }
  }
}

