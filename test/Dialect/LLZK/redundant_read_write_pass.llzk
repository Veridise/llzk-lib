module attributes {veridise.lang = "llzk"} {
  llzk.struct @Component {
    field @arr : !array.array<3 x !llzk.felt>

    function.def @compute(%arg : !llzk.felt, %inputIdx : index) -> !llzk.struct<@Component> {
      %self = new_struct : !llzk.struct<@Component>
      %arr = readf %self[@arr] : !llzk.struct<@Component>, !array.array<3 x !llzk.felt>
      %idx = arith.constant 1 : index
      %idx2 = arith.constant 2 : index
      array.write %arr[%idx] = %arg : !array.array<3 x !llzk.felt>, !llzk.felt
      array.write %arr[%inputIdx] = %arg : !array.array<3 x !llzk.felt>, !llzk.felt
      // this read is necessary in the case that %inputIdx == %idx
      %elem1 = array.read %arr[%idx] : !array.array<3 x !llzk.felt>, !llzk.felt
      %elem2 = array.read %arr[%inputIdx] : !array.array<3 x !llzk.felt>, !llzk.felt
      array.write %arr[%idx] = %elem2 : !array.array<3 x !llzk.felt>, !llzk.felt
      array.write %arr[%idx2] = %elem1 : !array.array<3 x !llzk.felt>, !llzk.felt
      function.return %self : !llzk.struct<@Component>
    }
    function.def @constrain(%self: !llzk.struct<@Component>, %arg : !llzk.felt, %inputIdx : index) { function.return }
  }
}

