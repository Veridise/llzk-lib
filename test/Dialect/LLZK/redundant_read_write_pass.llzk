module attributes {veridise.lang = "llzk"} {
  // A modified version of IsZero that includes some redundancy
  struct.def @IsZero {
    struct.field @bit : !felt.felt
    struct.field @zero : !felt.felt
    struct.field @one : !felt.felt

    function.def @compute(%a: !felt.felt) -> !struct.struct<@IsZero> {
      %self = struct.new : !struct.struct<@IsZero>
      %z = felt.const 0
      %o = felt.const 1
      struct.writef %self[@zero] = %z : !struct.struct<@IsZero>, !felt.felt
      struct.writef %self[@one] = %o : !struct.struct<@IsZero>, !felt.felt
      %b = llzk.cmp eq(%a, %z)
      %0 = scf.if %b -> !felt.felt {
        %z1 = struct.readf %self[@zero] : !struct.struct<@IsZero>, !felt.felt
        scf.yield %z1 : !felt.felt
      } else {
        %o1 = struct.readf %self[@one] : !struct.struct<@IsZero>, !felt.felt
        scf.yield %o1 : !felt.felt
      }
      struct.writef %self[@bit] = %0 : !struct.struct<@IsZero>, !felt.felt
      function.return %self : !struct.struct<@IsZero>
    }

    function.def @constrain(%self : !struct.struct<@IsZero>, %a: !felt.felt) {
      %0 = struct.readf %self[@bit] : !struct.struct<@IsZero>, !felt.felt
      %1 = felt.const 1
      %2 = felt.sub %0, %1
      %3 = felt.mul %0, %2
      %4 = felt.const 0
      llzk.emit_eq %3, %4 : !felt.felt
      %z1 = struct.readf %self[@zero] : !struct.struct<@IsZero>, !felt.felt
      %o1 = struct.readf %self[@one] : !struct.struct<@IsZero>, !felt.felt
      llzk.emit_eq %z1, %4 : !felt.felt
      llzk.emit_eq %o1, %1 : !felt.felt
      function.return
    }
  }
}

