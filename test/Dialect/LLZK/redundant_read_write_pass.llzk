// CHECK-NEXT:    field @one : !llzk.felt
// CHECK-NEXT:    function.def @compute(%[[VAL_0:.*]]: !llzk.felt) -> !llzk.struct<@IsZero> {
// CHECK-NEXT:      %[[VAL_1:.*]] = new_struct : <@IsZero>
// CHECK-NEXT:      %[[VAL_2:.*]] = constfelt  0
// CHECK-NEXT:      %[[VAL_3:.*]] = constfelt  1
