module attributes {veridise.lang = "llzk"} {
  llzk.struct @Component {
    field @matrix : !array.array<7,3 x !llzk.felt>

    function.def @compute(%inputArr : !array.array<3 x !llzk.felt>, %cond : i1) -> !llzk.struct<@Component> {
      %self = new_struct : !llzk.struct<@Component>
      %mat = readf %self[@matrix] : !llzk.struct<@Component>, !array.array<7,3 x !llzk.felt>
      %idx1 = arith.constant 1 : index
      %idx4 = arith.constant 4 : index

      scf.if %cond {
        array.insert %mat[%idx1] = %inputArr : !array.array<7,3 x !llzk.felt>, !array.array<3 x !llzk.felt>
      } else {
        %arr1 = array.extract %mat[%idx1] : !array.array<7,3 x !llzk.felt> // This load is necessary, as the value is unknown in this branch
        array.insert %mat[%idx4] = %arr1 : !array.array<7,3 x !llzk.felt>, !array.array<3 x !llzk.felt>
        array.insert %mat[%idx1] = %arr1 : !array.array<7,3 x !llzk.felt>, !array.array<3 x !llzk.felt> // this store is still unnecessary
      }

      function.return %self : !llzk.struct<@Component>
    }
    function.def @constrain(%self: !llzk.struct<@Component>, %inputArr : !array.array<3 x !llzk.felt>, %cond : i1) {
      function.return
    }
  }
}

