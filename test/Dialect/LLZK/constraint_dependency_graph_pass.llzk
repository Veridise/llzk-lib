// Testing external calls

module attributes {veridise.lang = "llzk"} {

  function.def private @extern_add(%a: !felt.felt, %b: !felt.felt) -> !felt.felt attributes {extern}

  llzk.struct @ExternAdder {
    llzk.field @sum : !felt.felt {llzk.pub}

    function.def @compute(%a: !felt.felt, %b: !felt.felt) -> !llzk.struct<@ExternAdder> {
      %self = llzk.new_struct : !llzk.struct<@ExternAdder>
      function.return %self : !llzk.struct<@ExternAdder>
    }

    function.def @constrain(%self: !llzk.struct<@ExternAdder>, %a: !felt.felt, %b: !felt.felt) {
      %sum = llzk.readf %self[@sum] : !llzk.struct<@ExternAdder>, !felt.felt
      %c = function.call @extern_add(%a, %b) : (!felt.felt, !felt.felt) -> (!felt.felt)
      // We make no assumptions about what %c could be, so this constraint is
      // effectively `%sum === {}`, so the CDG will be empty.
      llzk.emit_eq %sum, %c : !felt.felt
      function.return
    }
  }
}

// CHECK-LABEL: @ExternAdder ConstraintDependencyGraph { }

// -----

module attributes {veridise.lang = "llzk"} {

  function.def @global_add(%a: !felt.felt, %b: !felt.felt) -> !felt.felt {
    %c = felt.add %a, %b
    function.return %c : !felt.felt
  }

  function.def private @irrelevant() attributes {extern}

  llzk.struct @Adder2 {
    llzk.field @sum : !felt.felt {llzk.pub}

    function.def @compute(%a: !felt.felt, %b: !felt.felt) -> !llzk.struct<@Adder2> {
      %self = llzk.new_struct : !llzk.struct<@Adder2>
      %sum = function.call @global_add(%a, %b) : (!felt.felt, !felt.felt) -> (!felt.felt)
      llzk.writef %self[@sum] = %sum : !llzk.struct<@Adder2>, !felt.felt
      function.return %self : !llzk.struct<@Adder2>
    }

    function.def @constrain(%self: !llzk.struct<@Adder2>, %a: !felt.felt, %b: !felt.felt) {
      %sum = llzk.readf %self[@sum] : !llzk.struct<@Adder2>, !felt.felt
      %c = function.call @global_add(%a, %b) : (!felt.felt, !felt.felt) -> (!felt.felt)
      %d = felt.add %c, %c
      function.call @irrelevant() : () -> () // testing to see that this doesn't erase the prior values
      llzk.emit_eq %sum, %d : !felt.felt
      function.return
    }
  }
}

// CHECK-LABEL: @Adder2 ConstraintDependencyGraph {
// CHECK-NEXT:     { %arg0[@sum], %arg1, %arg2 }
// CHECK-NEXT: }

