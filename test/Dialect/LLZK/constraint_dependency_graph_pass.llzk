module attributes {veridise.lang = "llzk"} {
  // Constrains arr[1] === 7, leaving other indices unconstrained
  llzk.struct @ArrayConstrain {
    function.def @compute(%arr: !array.array<3 x !felt.felt>) -> !llzk.struct<@ArrayConstrain> {
      %self = llzk.new_struct : !llzk.struct<@ArrayConstrain>
      function.return %self : !llzk.struct<@ArrayConstrain>
    }

    function.def @constrain(%self: !llzk.struct<@ArrayConstrain>, %arr: !array.array<3 x !felt.felt>) {
      %const = felt.const 7
      %1 = arith.constant 1 : index
      %elem = array.read %arr[%1] : !array.array<3 x !felt.felt>, !felt.felt
      llzk.emit_eq %elem, %const : !felt.felt
      function.return
    }
  }

  llzk.struct @MatrixConstrain {
    llzk.field @check : !llzk.struct<@ArrayConstrain>

    llzk.field @a : !felt.felt
    llzk.field @b : !felt.felt
    llzk.field @c : !felt.felt
    llzk.field @d : !felt.felt
    llzk.field @e : !felt.felt
    llzk.field @f : !felt.felt

    function.def @compute() -> !llzk.struct<@MatrixConstrain> {
      %self = llzk.new_struct : !llzk.struct<@MatrixConstrain>
      function.return %self : !llzk.struct<@MatrixConstrain>
    }

    function.def @constrain(%self: !llzk.struct<@MatrixConstrain>) {
      %a = llzk.readf %self[@a] : !llzk.struct<@MatrixConstrain>, !felt.felt
      %b = llzk.readf %self[@b] : !llzk.struct<@MatrixConstrain>, !felt.felt
      %c = llzk.readf %self[@c] : !llzk.struct<@MatrixConstrain>, !felt.felt
      %d = llzk.readf %self[@d] : !llzk.struct<@MatrixConstrain>, !felt.felt
      %e = llzk.readf %self[@e] : !llzk.struct<@MatrixConstrain>, !felt.felt
      %f = llzk.readf %self[@f] : !llzk.struct<@MatrixConstrain>, !felt.felt

      %mat = array.new %a, %b, %c, %d, %e, %f : !array.array<2,3 x !felt.felt>

      %check = llzk.readf %self[@check] : !llzk.struct<@MatrixConstrain>, !llzk.struct<@ArrayConstrain>
      %0 = arith.constant 0 : index
      %arr = array.extract %mat[%0] : !array.array<2,3 x !felt.felt>
      function.call @ArrayConstrain::@constrain(%check, %arr) : (!llzk.struct<@ArrayConstrain>, !array.array<3 x !felt.felt>) -> ()

      function.return
    }
  }
}

// CHECK-LABEL: @ArrayConstrain ConstraintDependencyGraph {
// CHECK-NEXT:     { %arg1[1], <felt.const: 7> }
// CHECK-NEXT: }
// CHECK-NEXT: @MatrixConstrain ConstraintDependencyGraph {
// CHECK-NEXT:     { %arg0[@b], <felt.const: 7> }
// CHECK-NEXT: }

