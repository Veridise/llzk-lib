module attributes {veridise.lang = "llzk"} {
  // Constrains arr[0] === @A and arr[2] === @B, leaving arr[1] unconstrained
  llzk.struct @ArrayConstrain<[@A, @B]> {
    function.def @compute(%arr: !array.array<3 x !felt.felt>) -> !llzk.struct<@ArrayConstrain<[@A, @B]>> {
      %self = llzk.new_struct : !llzk.struct<@ArrayConstrain<[@A, @B]>>
      function.return %self : !llzk.struct<@ArrayConstrain<[@A, @B]>>
    }

    function.def @constrain(%self: !llzk.struct<@ArrayConstrain<[@A, @B]>>, %arr: !array.array<3 x !felt.felt>) {
      %const0 = llzk.read_const @A : !felt.felt
      %const2 = llzk.read_const @B : !felt.felt
      %0 = arith.constant 0 : index
      %2 = arith.constant 2 : index
      %elem0 = array.read %arr[%0] : !array.array<3 x !felt.felt>, !felt.felt
      %elem2 = array.read %arr[%2] : !array.array<3 x !felt.felt>, !felt.felt
      llzk.emit_eq %elem0, %const0 : !felt.felt
      llzk.emit_eq %elem2, %const2 : !felt.felt
      function.return
    }
  }

  llzk.struct @MatrixConstrain {
    llzk.field @check0 : !llzk.struct<@ArrayConstrain<[7, 11]>>
    llzk.field @check1 : !llzk.struct<@ArrayConstrain<[13, 17]>>

    function.def @compute(%mat: !array.array<2,3 x !felt.felt>) -> !llzk.struct<@MatrixConstrain> {
      %self = llzk.new_struct : !llzk.struct<@MatrixConstrain>
      function.return %self : !llzk.struct<@MatrixConstrain>
    }

    function.def @constrain(%self: !llzk.struct<@MatrixConstrain>, %mat: !array.array<2,3 x !felt.felt>) {
      // split into two arrays and constrain each
      %check0 = llzk.readf %self[@check0] : !llzk.struct<@MatrixConstrain>, !llzk.struct<@ArrayConstrain<[7, 11]>>
      %0 = arith.constant 0 : index
      %arr0 = array.extract %mat[%0] : !array.array<2,3 x !felt.felt>
      function.call @ArrayConstrain::@constrain(%check0, %arr0) : (!llzk.struct<@ArrayConstrain<[7, 11]>>, !array.array<3 x !felt.felt>) -> ()

      %check1 = llzk.readf %self[@check1] : !llzk.struct<@MatrixConstrain>, !llzk.struct<@ArrayConstrain<[13, 17]>>
      %1 = arith.constant 1 : index
      %arr1 = array.extract %mat[%1] : !array.array<2,3 x !felt.felt>
      function.call @ArrayConstrain::@constrain(%check1, %arr1) : (!llzk.struct<@ArrayConstrain<[13, 17]>>, !array.array<3 x !felt.felt>) -> ()
      function.return
    }
  }
}

