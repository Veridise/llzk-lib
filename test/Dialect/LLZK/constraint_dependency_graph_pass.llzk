module attributes {veridise.lang = "llzk"} {

  llzk.struct @EnsureZero {
    function.def @compute(%a: !felt.felt) -> !llzk.struct<@EnsureZero> {
      %self = llzk.new_struct : !llzk.struct<@EnsureZero>
      function.return %self : !llzk.struct<@EnsureZero>
    }

    function.def @constrain(%self: !llzk.struct<@EnsureZero>, %a: !felt.felt) {
      %zero = felt.const 0
      llzk.emit_eq %a, %zero : !felt.felt
      function.return
    }
  }

  llzk.struct @EnsureBothZero {
    function.def @compute(%a: !felt.felt, %b: !felt.felt) -> !llzk.struct<@EnsureBothZero> {
      %self = llzk.new_struct : !llzk.struct<@EnsureBothZero>
      function.return %self : !llzk.struct<@EnsureBothZero>
    }

    // NOTE: This test case demonstrates where this analysis is currently overly conservative,
    // as it does not determine that %a and %b must be precisely equal to 0.
    function.def @constrain(%self: !llzk.struct<@EnsureBothZero>, %a: !felt.felt, %b: !felt.felt) {
      %zero = felt.const 0
      llzk.emit_eq %a, %b : !felt.felt
      llzk.emit_eq %a, %zero : !felt.felt
      function.return
    }
  }
}

