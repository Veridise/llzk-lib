module attributes {veridise.lang = "llzk"} {

  function.def @global_add(%a: !felt.felt, %b: !felt.felt) -> !felt.felt {
    %c = felt.add %a, %b
    function.return %c : !felt.felt
  }

  llzk.struct @Adder {
    llzk.field @sum : !felt.felt {llzk.pub}

    function.def @compute(%a: !felt.felt, %b: !felt.felt) -> !llzk.struct<@Adder> {
      %self = llzk.new_struct : !llzk.struct<@Adder>
      %sum = function.call @global_add(%a, %b) : (!felt.felt, !felt.felt) -> (!felt.felt)
      llzk.writef %self[@sum] = %sum : !llzk.struct<@Adder>, !felt.felt
      function.return %self : !llzk.struct<@Adder>
    }

    function.def @constrain(%self: !llzk.struct<@Adder>, %a: !felt.felt, %b: !felt.felt) {
      %sum = llzk.readf %self[@sum] : !llzk.struct<@Adder>, !felt.felt
      %c = function.call @global_add(%a, %b) : (!felt.felt, !felt.felt) -> (!felt.felt)
      llzk.emit_eq %sum, %c : !felt.felt
      function.return
    }
  }
}

// CHECK-LABEL: @Adder ConstraintDependencyGraph {
// CHECK-NEXT:     { %arg0[@sum], %arg1, %arg2 }
// CHECK-NEXT: }

