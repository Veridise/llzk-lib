module attributes {veridise.lang = "llzk"} {
  llzk.struct @NoConstraints {
    function.def @compute(%a: !felt.felt) -> !llzk.struct<@NoConstraints> {
      %self = llzk.new_struct : !llzk.struct<@NoConstraints>
      function.return %self : !llzk.struct<@NoConstraints>
    }

    function.def @constrain(%self: !llzk.struct<@NoConstraints>, %a: !felt.felt) {
      function.return
    }
  }
}

