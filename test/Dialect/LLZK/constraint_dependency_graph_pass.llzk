module attributes {veridise.lang = "llzk"} {

  llzk.struct @Passthrough {
    llzk.field @out : !felt.felt {llzk.pub}

    function.def @compute(%in: !felt.felt) -> !llzk.struct<@Passthrough> {
      %self = llzk.new_struct : !llzk.struct<@Passthrough>
      llzk.writef %self[@out] = %in : !llzk.struct<@Passthrough>, !felt.felt
      function.return %self : !llzk.struct<@Passthrough>
    }

    function.def @constrain(%self: !llzk.struct<@Passthrough>, %in: !felt.felt) {
      %out = llzk.readf %self[@out] : !llzk.struct<@Passthrough>, !felt.felt
      llzk.emit_eq %in, %out : !felt.felt
      function.return
    }
  }

  llzk.struct @EnsureIsZero {
    llzk.field @p : !llzk.struct<@Passthrough>

    function.def @compute(%in: !felt.felt) -> !llzk.struct<@EnsureIsZero> {
      %self = llzk.new_struct : !llzk.struct<@EnsureIsZero>
      %p_0 = function.call @Passthrough::@compute(%in) : (!felt.felt) -> (!llzk.struct<@Passthrough>)
      llzk.writef %self[@p] = %p_0 : !llzk.struct<@EnsureIsZero>, !llzk.struct<@Passthrough>
      function.return %self : !llzk.struct<@EnsureIsZero>
    }

    function.def @constrain(%self: !llzk.struct<@EnsureIsZero>, %in: !felt.felt) {
      %zero = felt.const 0
      %p_0 = llzk.readf %self[@p] : !llzk.struct<@EnsureIsZero>, !llzk.struct<@Passthrough>
      function.call @Passthrough::@constrain(%p_0, %zero) : (!llzk.struct<@Passthrough>, !felt.felt) -> ()
      %out = llzk.readf %p_0[@out] : !llzk.struct<@Passthrough>, !felt.felt
      llzk.emit_eq %in, %out : !felt.felt
      function.return
    }
  }
}

