// Test that allowing Signals to emit_eq doesn't break the constraint analysis
!Signal = !llzk.struct<@Signal>
module attributes {veridise.lang = "llzk"} {
  // pre-defined
  llzk.struct @Signal {
    llzk.field @reg : !felt.felt {llzk.pub}

    function.def @compute(%v: !felt.felt) -> !llzk.struct<@Signal> {
      %self = llzk.new_struct : !llzk.struct<@Signal>
      llzk.writef %self[@reg] = %v : !llzk.struct<@Signal>, !felt.felt
      function.return %self : !llzk.struct<@Signal>
    }

    function.def @constrain(%self: !llzk.struct<@Signal>, %v: !felt.felt) {
      function.return
    }
  }

  llzk.struct @Component00 {
    llzk.field @f : !Signal

    function.def @compute(%0: !Signal) -> !llzk.struct<@Component00> {
      %self = llzk.new_struct : !llzk.struct<@Component00>
      llzk.writef %self[@f] = %0 : !llzk.struct<@Component00>, !Signal
      function.return %self : !llzk.struct<@Component00>
    }
    function.def @constrain(%self: !llzk.struct<@Component00>, %0: !Signal) {
      %b = llzk.readf %self[@f] : !llzk.struct<@Component00>, !Signal
      // Signal is the only struct allowed for emit equal
      llzk.emit_eq %b, %0 : !Signal
      function.return
    }
  }

  llzk.struct @Component01 {
    llzk.field @f : !array.array<2 x !Signal>

    function.def @compute(%0: !array.array<2 x !Signal>) -> !llzk.struct<@Component01> {
      %self = llzk.new_struct : !llzk.struct<@Component01>
      llzk.writef %self[@f] = %0 : !llzk.struct<@Component01>, !array.array<2 x !Signal>
      function.return %self : !llzk.struct<@Component01>
    }
    function.def @constrain(%self: !llzk.struct<@Component01>, %0: !array.array<2 x !Signal>) {
      %b = llzk.readf %self[@f] : !llzk.struct<@Component01>, !array.array<2 x !Signal>
      // Signal is the only struct allowed for emit equal
      llzk.emit_eq %b, %0 : !array.array<2 x !Signal>
      function.return
    }
  }
}

