module attributes {veridise.lang = "llzk"} {
  struct.def @ComponentD1 {
    struct.field @f1 : !felt.felt

    function.def @constrain(%self: !struct.struct<@ComponentD1>, %z: !felt.felt) { function.return }
    function.def @compute(%z: !felt.felt) -> !struct.struct<@ComponentD1> {
      %self = struct.new : !struct.struct<@ComponentD1>
      struct.writef %self[@f1] = %z : !struct.struct<@ComponentD1>, !felt.felt
      function.return %self : !struct.struct<@ComponentD1>
    }
  }

  struct.def @ComponentD2 {
    struct.field @f2 : !struct.struct<@ComponentD1>

    function.def @compute(%z: !felt.felt) -> !struct.struct<@ComponentD2> {
      %self = struct.new : !struct.struct<@ComponentD2>
      %q = function.call @ComponentD1::@compute(%z) : (!felt.felt) -> !struct.struct<@ComponentD1>
      struct.writef %self[@f2] = %q : !struct.struct<@ComponentD2>, !struct.struct<@ComponentD1>
      function.return %self : !struct.struct<@ComponentD2>
    }

    function.def @constrain(%self: !struct.struct<@ComponentD2>, %z: !felt.felt) {
      %q = struct.readf %self[@f2] : !struct.struct<@ComponentD2>, !struct.struct<@ComponentD1>
      function.call @ComponentD1::@constrain(%q, %z) : (!struct.struct<@ComponentD1>,!felt.felt) -> ()
      function.return
    }
  }
}
