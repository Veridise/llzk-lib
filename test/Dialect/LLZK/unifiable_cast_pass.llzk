// RUN: llzk-opt -split-input-file %s 2>&1 | FileCheck --enable-var-scope %s

// This IR mimics an idiom that could be generated by the zirgen frontend
#map = affine_map<()[s0, s1] -> (s0 + s1)>
module attributes { veridise.lang = "llzk" } {
  llzk.struct @A<[@X, @Y, @N]> {
    function.def @compute(%0: !array.array<@N x !felt.felt>) -> !llzk.struct<@A<[@X, @Y, @N]>> {
      %self = new_struct : <@A<[@X, @Y, @N]>>
      function.return %self : !llzk.struct<@A<[@X, @Y, @N]>>
    }
    function.def @constrain(%self: !llzk.struct<@A<[@X, @Y, @N]>>, %0: !array.array<@N x !felt.felt>) {
      function.return
    }
  }
  llzk.struct @B<[@X, @Y, @N]> {
    field @a : !llzk.struct<@A<[@X, @Y, #map]>>
    function.def @compute(%a0: !array.array<@N x !felt.felt>) -> !llzk.struct<@B<[@X, @Y, @N]>> {
      %self = new_struct : <@B<[@X, @Y, @N]>>
      %0 = read_const @X : index
      %1 = read_const @Y : index
      %2 = unifiable_cast %a0 : (!array.array<@N x !felt.felt>) -> !array.array<#map x !felt.felt>
      %3 = function.call @A::@compute(%2){()[%0, %1]} : (!array.array<#map x !felt.felt>) -> !llzk.struct<@A<[@X, @Y, #map]>>
      writef %self[@a] = %3 : !llzk.struct<@B<[@X, @Y, @N]>>, !llzk.struct<@A<[@X, @Y, #map]>>
      function.return %self : !llzk.struct<@B<[@X, @Y, @N]>>
    }
    function.def @constrain(%self: !llzk.struct<@B<[@X, @Y, @N]>>, %a0: !array.array<@N x !felt.felt>) {
      %2 = unifiable_cast %a0 : (!array.array<@N x !felt.felt>) -> !array.array<#map x !felt.felt>
      %a = readf %self[@a] : !llzk.struct<@B<[@X, @Y, @N]>>, !llzk.struct<@A<[@X, @Y, #map]>>
      function.call @A::@constrain(%a, %2) : (!llzk.struct<@A<[@X, @Y, #map]>>, !array.array<#map x !felt.felt>) -> ()
      function.return
    }
  }
}
