#map1 = affine_map<()[s0, s1] -> (s0 + s1)>
#map2 = affine_map<()[s0, s1, s2] -> (s0 + s1 + s2)>
module attributes {veridise.lang = "llzk"} {
  llzk.struct @T1<[]> {
    function.def @compute() -> !llzk.struct<@T1<[]>> {
      %self = new_struct : <@T1<[]>>
      function.return %self : !llzk.struct<@T1<[]>>
    }
    function.def @constrain(%self : !llzk.struct<@T1<[]>>) {
      function.return
    }
  }
  llzk.struct @T2<[@N]> {
    function.def @compute() -> !llzk.struct<@T2<[@N]>> {
      %self = new_struct : <@T2<[@N]>>
      function.return %self : !llzk.struct<@T2<[@N]>>
    }
    function.def @constrain(%self : !llzk.struct<@T2<[@N]>>) {
      function.return
    }
  }
  llzk.struct @T3<[@A, @B, @C, @D, @E]> {
    function.def @compute() -> !llzk.struct<@T3<[@A, @B, @C, @D, @E]>> {
      %self = new_struct : <@T3<[@A, @B, @C, @D, @E]>>
      function.return %self : !llzk.struct<@T3<[@A, @B, @C, @D, @E]>>
    }
    function.def @constrain(%self : !llzk.struct<@T3<[@A, @B, @C, @D, @E]>>) {
      function.return
    }
  }
  llzk.struct @Tester<[@S0, @S1]> {
    function.def @compute(%0: index, %1: i1, %2: !felt.felt, %3: !llzk.struct<@T1<[]>>,
                  %4: !llzk.struct<@T2<[1]>>, %5: !llzk.struct<@T2<[@S0]>>,
                  %6: !llzk.struct<@T2<[7]>>, %7: !llzk.struct<@T2<[!felt.felt]>>,
                  %8: !array.array<2,2 x !felt.felt>, %9: !array.array<2,@S0 x !felt.felt>,
                  %10: !array.array<2,7 x !felt.felt>, %11: !string.string, %12: !llzk.tvar<@S1>,
                  %18: !llzk.struct<@T3<[1, 7, @S0, 1, !llzk.struct<@T2<[1]>>]>>,
                  %20: !array.array<1, 7, @S0, 1 x !llzk.struct<@T2<[1]>>>) -> !llzk.struct<@Tester<[@S0, @S1]>> {
      %self = new_struct : <@Tester<[@S0, @S1]>>
      // Tests for equality
      %r0 = unifiable_cast %0 : (index) -> index
      %r1 = unifiable_cast %1 : (i1) -> i1
      %r2 = unifiable_cast %2 : (!felt.felt) -> !felt.felt
      %r3 = unifiable_cast %3 : (!llzk.struct<@T1<[]>>) -> !llzk.struct<@T1<[]>>
      %r4 = unifiable_cast %4 : (!llzk.struct<@T2<[1]>>) -> !llzk.struct<@T2<[1]>>
      %r5 = unifiable_cast %5 : (!llzk.struct<@T2<[@S0]>>) -> !llzk.struct<@T2<[@S0]>>
      %r6 = unifiable_cast %6 : (!llzk.struct<@T2<[7]>>) -> !llzk.struct<@T2<[#map1]>>
      %r7 = unifiable_cast %7 : (!llzk.struct<@T2<[!felt.felt]>>) -> !llzk.struct<@T2<[!felt.felt]>>
      %r8 = unifiable_cast %8 : (!array.array<2,2 x !felt.felt>) -> !array.array<2,2 x !felt.felt>
      %r9 = unifiable_cast %9 : (!array.array<2,@S0 x !felt.felt>) -> !array.array<2,@S0 x !felt.felt>
      %r10 = unifiable_cast %10 : (!array.array<2,7 x !felt.felt>) -> !array.array<2,#map1 x !felt.felt>
      %r11 = unifiable_cast %11 : (!string.string) -> !string.string
      // Test that a type variable unifies with any type
      %r12 = unifiable_cast %12 : (!llzk.tvar<@S1>) -> index
      %13 = unifiable_cast %2 : (!felt.felt) -> !llzk.tvar<@S1>
      // Test that if structs have the same name they unify if their params unify
      %19 = unifiable_cast %18 : (!llzk.struct<@T3<[1, 7, @S0, 1, !llzk.struct<@T2<[1]>>]>>) -> !llzk.struct<@T3<[#map1, #map1, 1, @S0, !llzk.struct<@T2<[#map2]>>]>>
      %21 = unifiable_cast %20 : (!array.array<1, 7, @S0, 1 x !llzk.struct<@T2<[1]>>>) -> !array.array<#map1, #map1, 1, @S0 x !llzk.struct<@T2<[#map2]>>>
      function.return %self : !llzk.struct<@Tester<[@S0, @S1]>>
    }
    function.def @constrain(%self: !llzk.struct<@Tester<[@S0, @S1]>>, %0: index, %1: i1, %2: !felt.felt, %3: !llzk.struct<@T1<[]>>,
                  %4: !llzk.struct<@T2<[1]>>, %5: !llzk.struct<@T2<[@S0]>>,
                  %6: !llzk.struct<@T2<[7]>>, %7: !llzk.struct<@T2<[!felt.felt]>>,
                  %8: !array.array<2,2 x !felt.felt>, %9: !array.array<2,@S0 x !felt.felt>,
                  %10: !array.array<2,7 x !felt.felt>, %11: !string.string, %12: !llzk.tvar<@S1>,
                  %18: !llzk.struct<@T3<[1, 7, @S0, 1, !llzk.struct<@T2<[1]>>]>>,
                  %20: !array.array<1, 7, @S0, 1 x !llzk.struct<@T2<[1]>>>) {
      function.return
    }
  }
}

