module attributes {veridise.lang = "llzk"} {
  llzk.struct @ComponentA {
    function.def @compute(%a: !felt.felt, %d: !felt.felt) -> !llzk.struct<@ComponentA> {
      %self = llzk.new_struct : !llzk.struct<@ComponentA>
      %lb = arith.constant 0 : index
      %up = arith.constant 4 : index
      %step = arith.constant 1 : index
      %res = scf.for %iv = %lb to %up step %step
          iter_args(%sum = %d) -> !felt.felt {
        %1 = felt.add %a, %sum
        scf.yield %1 : !felt.felt
      }
      function.return %self : !llzk.struct<@ComponentA>
    }

    function.def @constrain(%self: !llzk.struct<@ComponentA>, %a: !felt.felt, %d: !felt.felt) {
      function.return
    }
  }
}
