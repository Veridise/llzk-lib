// -----
module attributes {veridise.lang = "llzk"} {
  struct.def @ComponentH {
    struct.field @f2 : !felt.felt // expected-note {{see existing symbol definition here}}
    // expected-error@+1 {{redefinition of symbol named 'f2'}}
    struct.field @f2 : !felt.felt

    function.def @compute() -> !struct.struct<@ComponentH> {
      %self = struct.new : !struct.struct<@ComponentH>
      function.return %self : !struct.struct<@ComponentH>
    }
    function.def @constrain(%self: !struct.struct<@ComponentH>)  { function.return }
  }
}
// -----
module attributes {veridise.lang = "llzk"} {
  struct.def @ComponentJ {
    function.def @compute() -> !struct.struct<@ComponentJ> {
      %self = struct.new : !struct.struct<@ComponentJ>
      function.return %self : !struct.struct<@ComponentJ>
    }
    function.def @constrain(%self: !struct.struct<@ComponentJ>)  { function.return }
  }

  function.def @user() {
    // expected-error@+2 {{symbol "@ComponentJ" references a 'struct.def' but expected a 'function.def'}}
    // expected-error@+1 {{expected 'function.def' named "@ComponentJ"}}
    function.call @ComponentJ() : () -> ()
    function.return
  }
}
