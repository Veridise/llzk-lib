module attributes {veridise.lang = "llzk"} {
  llzk.struct @ComponentQ1<[@Z]> { // expected-note {{type parameters must unify with parameters defined here}}
    func @compute() -> !llzk.struct<@ComponentQ1<[@Z]>> {
      %self = new_struct : !llzk.struct<@ComponentQ1<[@Z]>>
      return %self : !llzk.struct<@ComponentQ1<[@Z]>>
    }
    func @constrain(%self: !llzk.struct<@ComponentQ1<[@Z]>>)  { return }
  }
  llzk.struct @ComponentQ2 {
    // expected-error@+1 {{Cannot unify parameters of type '!llzk.struct<@ComponentQ1>' with parameters of 'llzk.struct' "@ComponentQ1<[@Z]>"}}
    llzk.field @f : !llzk.struct<@ComponentQ1>

    func @compute() -> !llzk.struct<@ComponentQ2> {
      %self = new_struct : !llzk.struct<@ComponentQ2>
      return %self : !llzk.struct<@ComponentQ2>
    }
    func @constrain(%self: !llzk.struct<@ComponentQ2>)  { return }
  }
}
