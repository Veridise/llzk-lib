module attributes {veridise.lang = "llzk"} {
  llzk.struct @Component01 {
    field @f : !llzk.array<2 x !llzk.felt>

    function.def @compute(%0: !llzk.array<2 x !llzk.felt>) -> !llzk.struct<@Component01> {
      %self = new_struct : !llzk.struct<@Component01>
      writef %self[@f] = %0 : !llzk.struct<@Component01>, !llzk.array<2 x !llzk.felt>
      function.return %self : !llzk.struct<@Component01>
    }
    function.def @constrain(%self: !llzk.struct<@Component01>, %0: !llzk.array<2 x !llzk.felt>) {
      %b = readf %self[@f] : !llzk.struct<@Component01>, !llzk.array<2 x !llzk.felt>
      emit_eq %0, %b : !llzk.array<2 x !llzk.felt>
      function.return
    }
  }

  llzk.struct @Component02<[@N]> {
    field @g : !llzk.array<@N x !llzk.struct<@Component01>>

    function.def @compute(%in: !llzk.array<@N,2 x !llzk.felt>) -> !llzk.struct<@Component02<[@N]>> {
      %self = new_struct : !llzk.struct<@Component02<[@N]>>
      %a = array.new : !llzk.array<@N x !llzk.struct<@Component01>>

      %lb = arith.constant 0 : index
      %up = read_const @N : index
      %step = arith.constant 1 : index
      scf.for %iv = %lb to %up step %step {
        %p = array.extract %in[%iv] : !llzk.array<@N,2 x !llzk.felt>
        %c = function.call @Component01::@compute(%p) : (!llzk.array<2 x !llzk.felt>) -> !llzk.struct<@Component01>
        array.write %a[%iv] = %c : !llzk.array<@N x !llzk.struct<@Component01>>, !llzk.struct<@Component01>
      }

      writef %self[@g] = %a : !llzk.struct<@Component02<[@N]>>, !llzk.array<@N x !llzk.struct<@Component01>>
      function.return %self : !llzk.struct<@Component02<[@N]>>
    }

    function.def @constrain(%self: !llzk.struct<@Component02<[@N]>>, %in: !llzk.array<@N,2 x !llzk.felt>) {
      %a = readf %self[@g] : !llzk.struct<@Component02<[@N]>>, !llzk.array<@N x !llzk.struct<@Component01>>

      %lb = arith.constant 0 : index
      %up = read_const @N : index
      %step = arith.constant 1 : index
      scf.for %iv = %lb to %up step %step {
        %p = array.extract %in[%iv] : !llzk.array<@N,2 x !llzk.felt>
        %c = array.read %a[%iv] : !llzk.array<@N x !llzk.struct<@Component01>>, !llzk.struct<@Component01>
        function.call @Component01::@constrain(%c, %p) : (!llzk.struct<@Component01>, !llzk.array<2 x !llzk.felt>) -> ()
      }
      function.return
    }
  }
}
