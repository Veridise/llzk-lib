function.def @constant_index() -> !llzk.array<2 x index> {
  %a = arith.constant 45 : index
  %b = arith.constant 214 : index
  %c = arith.constant 2 : index
  %d = arith.constant 52 : index
  %r = array.new %a, %b, %c, %d : !llzk.array<2,2 x index>
  %i = arith.constant 1 : index
  %s = array.extract %r[%i] : !llzk.array<2,2 x index>
  function.return %s: !llzk.array<2 x index>
}
