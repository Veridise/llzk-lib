// RUN: llzk-opt -split-input-file -verify-diagnostics %s | FileCheck --enable-var-scope %s

#map = affine_map<()[s0, s1] -> (s0 + s1)>
module attributes {veridise.lang = "llzk"} {
  llzk.struct @Component<[]> {
    function.def @compute() -> !llzk.struct<@Component<[]>> {
      %self = llzk.new_struct : !llzk.struct<@Component<[]>>
      function.return %self : !llzk.struct<@Component<[]>>
    }

    function.def @constrain(%self: !llzk.struct<@Component<[]>>) {
      function.return
    }
  }

  llzk.struct @Reg {
    // ZIR frontend should mark function.return value with `pub`
    llzk.field @reg : !felt.felt {llzk.pub}

    function.def @compute(%v: !felt.felt) -> !llzk.struct<@Reg> {
      %self = llzk.new_struct : !llzk.struct<@Reg>
      llzk.writef %self[@reg] = %v : !llzk.struct<@Reg>, !felt.felt
      function.return %self : !llzk.struct<@Reg>
    }

    function.def @constrain(%self: !llzk.struct<@Reg>, %v: !felt.felt) {
      %0 = llzk.readf %self[@reg] : !llzk.struct<@Reg>, !felt.felt
      llzk.emit_eq %v, %0 : !felt.felt
      function.return
    }
  }

  llzk.struct @Div<[]> {
    llzk.field @reciprocal : !felt.felt
    // ZIR frontend should mark function.return value with `pub`
    llzk.field @synthetic_return : !felt.felt {llzk.pub}

    function.def @compute(%lhs: !felt.felt, %rhs: !felt.felt) -> !llzk.struct<@Div<[]>> {
      %self = llzk.new_struct : !llzk.struct<@Div<[]>>
      // res_inv := inv(rhs);
      %res_inv = felt.inv %rhs
      llzk.writef %self[@reciprocal] = %res_inv : !llzk.struct<@Div<[]>>, !felt.felt
      // self.synthetic_return := res_inv * lhs;
      %res_mul = felt.mul %res_inv, %lhs
      llzk.writef %self[@synthetic_return] = %res_mul : !llzk.struct<@Div<[]>>, !felt.felt
      //
      function.return %self : !llzk.struct<@Div<[]>>
    }

    function.def @constrain(%self: !llzk.struct<@Div<[]>>, %lhs: !felt.felt, %rhs: !felt.felt) {
      // emit self.reciprocal * rhs = 1;
      %res_inv = llzk.readf %self[@reciprocal] : !llzk.struct<@Div<[]>>, !felt.felt
      %res_mul_1 = felt.mul %res_inv, %rhs
      %const_1 = felt.const 1
      llzk.emit_eq %res_mul_1, %const_1 : !felt.felt
      // emit self.synthetic_return = self.reciprocal * lhs;
      %res_self = llzk.readf %self[@synthetic_return] : !llzk.struct<@Div<[]>>, !felt.felt
      %res_mul_2 = felt.mul %res_inv, %lhs
      llzk.emit_eq %res_self, %res_mul_2 : !felt.felt
      //
      function.return
    }
  }

  llzk.struct @Foo<[@N, @P, @Aff$0]> {
    llzk.field @"$super" : !llzk.struct<@Component<[]>>
    llzk.field @"$temp_0" : !llzk.struct<@Component<[]>>
    llzk.field @"$temp" : !llzk.struct<@Div<[]>>
    function.def @compute(%arg0: !array.array<@Aff$0 x !felt.felt>) -> !llzk.struct<@Foo<[@N, @P, @Aff$0]>> {
      %self = llzk.new_struct : !llzk.struct<@Foo<[@N, @P, @Aff$0]>>
      %0 = llzk.read_const @N : !felt.felt
      %1 = llzk.read_const @P : !felt.felt
      %2 = function.call @Div::@compute(%0, %1) : (!felt.felt, !felt.felt) -> !llzk.struct<@Div<[]>>
      llzk.writef %self[@"$temp"] = %2 : !llzk.struct<@Foo<[@N, @P, @Aff$0]>>, !llzk.struct<@Div<[]>>
      %3 = llzk.readf %self[@"$temp"] : !llzk.struct<@Foo<[@N, @P, @Aff$0]>>, !llzk.struct<@Div<[]>>
      %4 = function.call @Component::@compute() : () -> !llzk.struct<@Component<[]>>
      llzk.writef %self[@"$temp_0"] = %4 : !llzk.struct<@Foo<[@N, @P, @Aff$0]>>, !llzk.struct<@Component<[]>>
      %5 = llzk.readf %self[@"$temp_0"] : !llzk.struct<@Foo<[@N, @P, @Aff$0]>>, !llzk.struct<@Component<[]>>
      llzk.writef %self[@"$super"] = %5 : !llzk.struct<@Foo<[@N, @P, @Aff$0]>>, !llzk.struct<@Component<[]>>
      function.return %self : !llzk.struct<@Foo<[@N, @P, @Aff$0]>>
    }
    function.def @constrain(%arg0: !llzk.struct<@Foo<[@N, @P, @Aff$0]>>, %arg1: !array.array<@Aff$0 x !felt.felt>) {
      %0 = llzk.read_const @N : !felt.felt
      %1 = llzk.read_const @P : !felt.felt
      %2 = llzk.readf %arg0[@"$temp"] : !llzk.struct<@Foo<[@N, @P, @Aff$0]>>, !llzk.struct<@Div<[]>>
      function.call @Div::@constrain(%2, %0, %1) : (!llzk.struct<@Div<[]>>, !felt.felt, !felt.felt) -> ()
      %3 = llzk.readf %arg0[@"$temp_0"] : !llzk.struct<@Foo<[@N, @P, @Aff$0]>>, !llzk.struct<@Component<[]>>
      function.call @Component::@constrain(%3) : (!llzk.struct<@Component<[]>>) -> ()
      function.return
    }
  }

  llzk.struct @Bar<[@N, @P, @Aff$0]> {
    llzk.field @"$super" : !llzk.struct<@Component<[]>>
    llzk.field @"$temp_1" : !llzk.struct<@Component<[]>>
    llzk.field @"$temp_0" : !llzk.struct<@Foo<[@N, @P, #map]>>
    llzk.field @"$temp" : !llzk.struct<@Div<[]>>
    function.def @compute(%arg0: !array.array<@Aff$0 x !felt.felt>) -> !llzk.struct<@Bar<[@N, @P, @Aff$0]>> {
      %self = llzk.new_struct : !llzk.struct<@Bar<[@N, @P, @Aff$0]>>
      %0 = llzk.read_const @N : !felt.felt
      %1 = llzk.read_const @P : !felt.felt
      %2 = function.call @Div::@compute(%0, %1) : (!felt.felt, !felt.felt) -> !llzk.struct<@Div<[]>>
      llzk.writef %self[@"$temp"] = %2 : !llzk.struct<@Bar<[@N, @P, @Aff$0]>>, !llzk.struct<@Div<[]>>
      %3 = llzk.readf %self[@"$temp"] : !llzk.struct<@Bar<[@N, @P, @Aff$0]>>, !llzk.struct<@Div<[]>>
      %4 = llzk.read_const @N : !felt.felt
      %5 = cast.toindex %4
      %6 = llzk.read_const @P : !felt.felt
      %7 = cast.toindex %6
      %8 = function.call @Foo::@compute(%arg0) {()[%5, %7]} : (!array.array<@Aff$0 x !felt.felt>) -> !llzk.struct<@Foo<[@N, @P, #map]>>
      llzk.writef %self[@"$temp_0"] = %8 : !llzk.struct<@Bar<[@N, @P, @Aff$0]>>, !llzk.struct<@Foo<[@N, @P, #map]>>
      %9 = llzk.readf %self[@"$temp_0"] : !llzk.struct<@Bar<[@N, @P, @Aff$0]>>, !llzk.struct<@Foo<[@N, @P, #map]>>
      %10 = function.call @Component::@compute() : () -> !llzk.struct<@Component<[]>>
      llzk.writef %self[@"$temp_1"] = %10 : !llzk.struct<@Bar<[@N, @P, @Aff$0]>>, !llzk.struct<@Component<[]>>
      %11 = llzk.readf %self[@"$temp_1"] : !llzk.struct<@Bar<[@N, @P, @Aff$0]>>, !llzk.struct<@Component<[]>>
      llzk.writef %self[@"$super"] = %11 : !llzk.struct<@Bar<[@N, @P, @Aff$0]>>, !llzk.struct<@Component<[]>>
      function.return %self : !llzk.struct<@Bar<[@N, @P, @Aff$0]>>
    }
    function.def @constrain(%arg0: !llzk.struct<@Bar<[@N, @P, @Aff$0]>>, %arg1: !array.array<@Aff$0 x !felt.felt>) {
      %0 = llzk.read_const @N : !felt.felt
      %1 = llzk.read_const @P : !felt.felt
      %2 = llzk.readf %arg0[@"$temp"] : !llzk.struct<@Bar<[@N, @P, @Aff$0]>>, !llzk.struct<@Div<[]>>
      function.call @Div::@constrain(%2, %0, %1) : (!llzk.struct<@Div<[]>>, !felt.felt, !felt.felt) -> ()
      %3 = llzk.readf %arg0[@"$temp_0"] : !llzk.struct<@Bar<[@N, @P, @Aff$0]>>, !llzk.struct<@Foo<[@N, @P, #map]>>
      function.call @Foo::@constrain(%3, %arg1) : (!llzk.struct<@Foo<[@N, @P, #map]>>, !array.array<@Aff$0 x !felt.felt>) -> ()
      %4 = llzk.readf %arg0[@"$temp_1"] : !llzk.struct<@Bar<[@N, @P, @Aff$0]>>, !llzk.struct<@Component<[]>>
      function.call @Component::@constrain(%4) : (!llzk.struct<@Component<[]>>) -> ()
      function.return
    }
  }
}

