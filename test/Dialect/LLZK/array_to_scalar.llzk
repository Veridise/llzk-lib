module attributes {veridise.lang = "llzk"} {
  function.def @new_then_write_then_read() -> !llzk.felt {
    %0 = arith.constant 0 : index
    %1 = arith.constant 1 : index

    %a = llzk.constfelt 0
    %b = llzk.constfelt 424867
    %c = llzk.constfelt 2465
    %d = llzk.constfelt 367

    %r = array.new %a, %a, %a, %a: !array.array<2,2 x !llzk.felt>
    array.write %r[%0,%1] = %b: !array.array<2,2 x !llzk.felt>, !llzk.felt
    array.write %r[%0,%0] = %c: !array.array<2,2 x !llzk.felt>, !llzk.felt
    array.write %r[%1,%0] = %d: !array.array<2,2 x !llzk.felt>, !llzk.felt
    // [[2465, 424867],[367, 0]]
    %w = array.read %r[%0,%0] : !array.array<2,2 x !llzk.felt>, !llzk.felt
    %x = array.read %r[%0,%1] : !array.array<2,2 x !llzk.felt>, !llzk.felt
    %y = array.read %r[%1,%0] : !array.array<2,2 x !llzk.felt>, !llzk.felt
    %z = array.read %r[%1,%1] : !array.array<2,2 x !llzk.felt>, !llzk.felt

    %s1 = llzk.add %w, %x   // 2465 + 424867
    %s2 = llzk.add %s1, %y  // ^ + 367
    %s3 = llzk.add %s2, %z  // ^ + 0
    function.return %s3 : !llzk.felt
  }
}
// CHECK-LABEL: function.def @new_then_write_then_read() -> !llzk.felt {
// CHECK-NEXT:    %[[V0:[0-9a-zA-Z_\.]+]] = constfelt  0
// CHECK-NEXT:    %[[V1:[0-9a-zA-Z_\.]+]] = constfelt  424867
// CHECK-NEXT:    %[[V2:[0-9a-zA-Z_\.]+]] = constfelt  2465
// CHECK-NEXT:    %[[V3:[0-9a-zA-Z_\.]+]] = constfelt  367
// CHECK-NEXT:    %[[V4:[0-9a-zA-Z_\.]+]] = add %[[V1]], %[[V2]] : !llzk.felt, !llzk.felt
// CHECK-NEXT:    %[[V5:[0-9a-zA-Z_\.]+]] = add %[[V4]], %[[V3]] : !llzk.felt, !llzk.felt
// CHECK-NEXT:    %[[V6:[0-9a-zA-Z_\.]+]] = add %[[V5]], %[[V0]] : !llzk.felt, !llzk.felt
// CHECK-NEXT:    function.return %[[V6]] : !llzk.felt
// CHECK-NEXT:  }
// -----

