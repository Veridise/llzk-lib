  llzk.struct @ArrayFields {
    llzk.field @arr : !array.array<2,3 x !F>

    function.def @compute(%a0: !F, %a1: !F, %a2: !F, %a3: !F, %a4: !F, %a5: !F) -> !llzk.struct<@ArrayFields> {
      %self = new_struct : !llzk.struct<@ArrayFields>
      %a = array.new %a0, %a1, %a2, %a3, %a4, %a5 : !array.array<2,3 x !F>
      writef %self[@arr] = %a : !llzk.struct<@ArrayFields>, !array.array<2,3 x !F>
      function.return %self : !llzk.struct<@ArrayFields>
    }

    function.def @constrain(%self: !llzk.struct<@ArrayFields>, %a0: !F, %a1: !F, %a2: !F, %a3: !F, %a4: !F, %a5: !F) {
      %a = readf %self[@arr] : !llzk.struct<@ArrayFields>, !array.array<2,3 x !F>

      %0 = arith.constant 0 : index
      %1 = arith.constant 1 : index
      %2 = arith.constant 2 : index

      %b0 = array.read %a[%0, %0] : !array.array<2,3 x !F>, !F
      emit_eq %a0, %b0 : !F

      %b1 = array.read %a[%0, %1] : !array.array<2,3 x !F>, !F
      emit_eq %a1, %b1 : !F

      %b2 = array.read %a[%0, %2] : !array.array<2,3 x !F>, !F
      emit_eq %a2, %b2 : !F

      %b3 = array.read %a[%1, %0] : !array.array<2,3 x !F>, !F
      emit_eq %a3, %b3 : !F

      %b4 = array.read %a[%1, %1] : !array.array<2,3 x !F>, !F
      emit_eq %a4, %b4 : !F

      %b5 = array.read %a[%1, %2] : !array.array<2,3 x !F>, !F
      emit_eq %a5, %b5 : !F

      function.return
    }
  }
}
