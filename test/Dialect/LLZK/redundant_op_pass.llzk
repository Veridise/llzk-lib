module attributes {veridise.lang = "llzk"} {
  llzk.struct @A {
    function.def @compute() -> !llzk.struct<@A> {
      %self = new_struct : !llzk.struct<@A>
      function.return %self : !llzk.struct<@A>
    }
    function.def @constrain(%self : !llzk.struct<@A>) {
      function.return
    }
  }

  llzk.struct @B {
    field @a : !llzk.struct<@A>
    function.def @compute() -> !llzk.struct<@B> {
      %self = new_struct : !llzk.struct<@B>
      function.return %self : !llzk.struct<@B>
    }
    function.def @constrain(%self : !llzk.struct<@B>) {
      %a = readf %self[@a] : !llzk.struct<@B>, !llzk.struct<@A>
      function.call @A::@constrain(%a) : (!llzk.struct<@A>) -> ()
      function.return
    }
  }

  llzk.struct @C {
    field @b : !llzk.struct<@B>
    function.def @compute() -> !llzk.struct<@C> {
      %self = new_struct : !llzk.struct<@C>
      function.return %self : !llzk.struct<@C>
    }
    function.def @constrain(%self : !llzk.struct<@C>) {
      %b = readf %self[@b] : !llzk.struct<@C>, !llzk.struct<@B>
      function.call @B::@constrain(%b) : (!llzk.struct<@B>) -> ()
      function.return
    }
  }

  llzk.struct @Main {
    field @c : !llzk.struct<@C>
    function.def @compute() -> !llzk.struct<@Main> {
      %self = new_struct : !llzk.struct<@Main>
      function.return %self : !llzk.struct<@Main>
    }
    function.def @constrain(%self : !llzk.struct<@Main>) {
      %c = readf %self[@c] : !llzk.struct<@Main>, !llzk.struct<@C>
      function.call @C::@constrain(%c) : (!llzk.struct<@C>) -> ()
      function.return
    }
  }
}

