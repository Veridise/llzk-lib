// RUN: llzk-opt -I %input_dir -split-input-file -llzk-inline-includes -llzk-duplicate-read-write-elim -llzk-duplicate-op-elim %s 2>&1 | FileCheck %s

module attributes {veridise.lang = "llzk"} {
  llzk.struct @Component {
    field @matrix : !array.array<7,3 x !llzk.felt>

    function.def @compute(%inputArr : !array.array<3 x !llzk.felt>) -> !llzk.struct<@Component> {
      %self = new_struct : !llzk.struct<@Component>
      %mat = readf %self[@matrix] : !llzk.struct<@Component>, !array.array<7,3 x !llzk.felt>
      %idx1 = arith.constant 1 : index
      %idx4 = arith.constant 4 : index
      array.insert %mat[%idx1] = %inputArr : !array.array<7,3 x !llzk.felt>, !array.array<3 x !llzk.felt>
      %arr1 = array.extract %mat[%idx1] : !array.array<7,3 x !llzk.felt> // redundant, replace with %inputArr
      array.insert %mat[%idx4] = %arr1 : !array.array<7,3 x !llzk.felt>, !array.array<3 x !llzk.felt> // replace %arr1 with %inputArr
      array.insert %mat[%idx1] = %arr1 : !array.array<7,3 x !llzk.felt>, !array.array<3 x !llzk.felt> // redundant
      function.return %self : !llzk.struct<@Component>
    }
    function.def @constrain(%self: !llzk.struct<@Component>, %inputArr : !array.array<3 x !llzk.felt>) {
      %mat = readf %self[@matrix] : !llzk.struct<@Component>, !array.array<7,3 x !llzk.felt>
      %i = arith.constant 2 : index
      %j = arith.constant 3 : index
      %k = arith.constant 3 : index // redundant constant
      %arr_i = array.extract %mat[%i] : !array.array<7,3 x !llzk.felt>
      %arr_j = array.extract %mat[%j] : !array.array<7,3 x !llzk.felt>
      %arr_k = array.extract %mat[%k] : !array.array<7,3 x !llzk.felt> // redundant extract
      emit_eq %arr_i, %arr_j : !array.array<3 x !llzk.felt>
      emit_eq %arr_j, %arr_k : !array.array<3 x !llzk.felt> // this constraint is unnecessary, ends comparing %mat[3] == %mat[3]
      emit_eq %arr_k, %arr_i : !array.array<3 x !llzk.felt> // this is the same as the first emit_eq op, just with LHS and RHS flipped
      function.return
    }
  }
}

// CHECK-LABEL: llzk.struct @Component {
// CHECK-NEXT:    field @matrix : !array.array<7,3 x !llzk.felt>
// CHECK-NEXT:    function.def @compute(%[[VAL_0:.*]]: !array.array<3 x !llzk.felt>) -> !llzk.struct<@Component> {
// CHECK-NEXT:      %[[VAL_1:.*]] = new_struct : <@Component>
// CHECK-NEXT:      %[[VAL_2:.*]] = readf %[[VAL_1]][@matrix] : <@Component>, !array.array<7,3 x !llzk.felt>
// CHECK-NEXT:      %[[VAL_3:.*]] = arith.constant 1 : index
// CHECK-NEXT:      %[[VAL_4:.*]] = arith.constant 4 : index
// CHECK-NEXT:      array.insert %[[VAL_2]]{{\[}}%[[VAL_3]]] = %[[VAL_0]] : <7,3 x !llzk.felt>, <3 x !llzk.felt>
// CHECK-NEXT:      array.insert %[[VAL_2]]{{\[}}%[[VAL_4]]] = %[[VAL_0]] : <7,3 x !llzk.felt>, <3 x !llzk.felt>
// CHECK-NEXT:      function.return %[[VAL_1]] : !llzk.struct<@Component>
// CHECK-NEXT:    }
// CHECK-NEXT:    function.def @constrain(%[[VAL_5:.*]]: !llzk.struct<@Component>, %[[VAL_6:.*]]: !array.array<3 x !llzk.felt>) {
// CHECK-NEXT:      %[[VAL_7:.*]] = readf %[[VAL_5]][@matrix] : <@Component>, !array.array<7,3 x !llzk.felt>
// CHECK-NEXT:      %[[VAL_8:.*]] = arith.constant 2 : index
// CHECK-NEXT:      %[[VAL_9:.*]] = arith.constant 3 : index
// CHECK-NEXT:      %[[VAL_10:.*]] = array.extract %[[VAL_7]]{{\[}}%[[VAL_8]]] : <7,3 x !llzk.felt>
// CHECK-NEXT:      %[[VAL_11:.*]] = array.extract %[[VAL_7]]{{\[}}%[[VAL_9]]] : <7,3 x !llzk.felt>
// CHECK-NEXT:      emit_eq %[[VAL_10]], %[[VAL_11]] : !array.array<3 x !llzk.felt>, !array.array<3 x !llzk.felt>
// CHECK-NEXT:      function.return
// CHECK-NEXT:    }
// CHECK-NEXT:  }

