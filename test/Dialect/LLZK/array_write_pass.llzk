// RUN: llzk-opt -split-input-file %s 2>&1 | FileCheck --enable-var-scope %s

function.def @arr_constant_index() -> !llzk.array<3 x index> {
  %a = arith.constant 45 : index
  %b = arith.constant 214 : index
  %c = arith.constant 2 : index
  %r = array.new %a, %b, %c : !llzk.array<3 x index>
  array.write %r[%c] = %b : !llzk.array<3 x index>, index
  function.return %r: !llzk.array<3 x index>
}
