function.def @arr_arr_mix_index(%a: index) -> !llzk.array<2,3 x index> {
  %c = arith.constant 343245 : index
  %d = arith.constant 345 : index
  %e = arith.constant 2344 : index
  %c1 = arith.constant 41987 : index
  %d1 = arith.constant 547 : index
  %e1 = arith.constant 54795 : index
  %i = arith.constant 1 : index
  %r = array.new %c, %d, %e, %c1, %d1, %e1: !llzk.array<2,3 x index>
  array.write %r[%i, %i] = %a : !llzk.array<2,3 x index>, index
  function.return %r: !llzk.array<2,3 x index>
}
