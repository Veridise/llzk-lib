//CHECK-NEXT:     %[[T3:[0-9a-zA-Z_\.]+]] = arith.constant 2 : index
//CHECK-NEXT:     %[[T4:[0-9a-zA-Z_\.]+]] = array.new %[[T1]], %[[T2]], %[[T3]] : <3 x index>
