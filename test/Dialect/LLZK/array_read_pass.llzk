function.def @arr_arr_mix_index(%a: !llzk.array<3 x index>) -> index {
  %c = arith.constant 343245 : index
  %d = arith.constant 345 : index
  %e = arith.constant 2344 : index
  %b = array.new %e, %c, %d: !llzk.array<3 x index>
  %i = arith.constant 1 : index
  %a1 = array.read %a[%i] : !llzk.array<3 x index>, index
  %a2 = array.read %a[%i] : !llzk.array<3 x index>, index
  %a3 = array.read %a[%i] : !llzk.array<3 x index>, index
  %r = array.new %a1, %a2, %a3, %c, %d, %e : !llzk.array<2,3 x index>
  %f = array.read %r[%i, %i] : !llzk.array<2,3 x index>, index
  function.return %f: index
}
