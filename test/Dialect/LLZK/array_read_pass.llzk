function.def @arr_arr_mix_index(%a: !llzk.array<3,4 x index>) -> index {
  %i0 = arith.constant 1 : index
  %i1 = arith.constant 3 : index
  %e = array.read %a[%i0, %i1] : !llzk.array<3,4 x index>, index
  function.return %e: index
}
