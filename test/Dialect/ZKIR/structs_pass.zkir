zkir.func @identity(%x: !zkir.felt) -> !zkir.felt {
  return %x: !zkir.felt
}

zkir.struct @ComponentH2 {
  func @compute(%self: !zkir.struct<@ComponentH2>) {
    return
  }

  func @constrain(%self: !zkir.struct<@ComponentH2>, %a: !zkir.felt) {
    call @identity(%a) : (!zkir.felt) -> (!zkir.felt)
    return
  }
}
