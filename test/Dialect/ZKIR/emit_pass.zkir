// RUN: zkir-opt -split-input-file %s 2>&1 | FileCheck %s

zkir.struct @ComponentA {
  func @constrain(%a: !zkir.felt, %b: !zkir.felt) {
    zkir.emit_eq %a, %b : !zkir.felt
    return
  }

  func @compute() {
    return
  }
}
