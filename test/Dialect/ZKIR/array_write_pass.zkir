//CHECK-NEXT:     %[[T3:[0-9a-zA-Z_\.]+]] = index.constant 2
//CHECK-NEXT:     %[[T4:[0-9a-zA-Z_\.]+]] = new_array %[[T1]], %[[T2]], %[[T3]] : <3 x index>
//CHECK-NEXT:     writearr %[[T4]][%[[T3]]] = %[[T2]] : <3 x index>, index
//CHECK-NEXT:     return %[[T4]] : !zkir.array<3 x index>
//CHECK-NEXT:   }
